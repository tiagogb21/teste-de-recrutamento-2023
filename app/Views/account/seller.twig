<html lang="pt-pt">
	<head>
		<meta charset="UTF-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>{{ title }}</title>
		<link rel="stylesheet" href="/assets/css/account/seller.css">
		<link rel="stylesheet" href="https://kit.fontawesome.com/860a93fa84.css" crossorigin="anonymous">
		<link rel="stylesheet" href="/assets/css/main.css">
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
	</head>
	<body class="">
		<div name="left-menu-area" class="">
			{{ header |raw }}
		</div>
		<div class="container pt-2 mb-5">
			<nav aria-label="breadcrumb">
				<ol class="breadcrumb">
					<li class="breadcrumb-item">
						<a href="#">Início</a>
					</li>
					<li class="breadcrumb-item">
						<a href="#">Minha conta</a>
					</li>
					<li class="breadcrumb-item active" aria-current="page">
						Área do Vendedor
					</li>
				</ol>
			</nav>
			<div class="row">
				<div name="left-menu-area" class="col-md-3">
					{{ left_menu |raw }}
				</div>
				<div name="right-card-area" class="col-md-9">
					<div class="card">
						<div class="card-body">
							<div name="title-area" class="col-md-12 mb-3">
								<h5>Área do Vendedor</h5>
							</div>
							<div name="tab-area" class="col-md-12 mb-3">
								<ul class="nav nav-tabs">
									{% for tab in tabs %}
										<li class="nav-item">
											<a {% if selected_tab == loop.index %} aria-current="page" class="nav-link active" {% else %} class="nav-link" {% endif %} href="{{ tab.link }}">
												{{ tab.label }}
											</a>
										</li>
									{% endfor %}
								</ul>
							</div>
							{{ tabBody |raw }}
						</div>
					</div>
				</div>
			</div>
		</div>
		<script src="https://kit.fontawesome.com/860a93fa84.js" crossorigin="anonymous"></script>
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
	</body>
</html>
